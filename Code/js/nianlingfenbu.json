{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A simple bar chart with embedded data.",
  "width": "500",
  "height": "400",
  "data": {
    "url": "../data/data1.csv"
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "bin": {
        "base": 7
      },
      "field": "age",
      "type": "ordinal"
    },
    "y": {
      "aggregate": "count"
    }
  }
}