{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A scatterplot showing horsepower and miles per gallons for various cars.",
    "width": "1100",
    "data": {
        "url": "../data/data2.csv"
    },
    "mark": "point",
    "encoding": {
        "x": {
            "field": "ID",
            "type": "quantitative",
            "scale": {"domain": [292881, 293785]}
